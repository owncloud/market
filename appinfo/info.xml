<?xml version="1.0"?>
<info>
	<id>market</id>
	<name>Market</name>
	<description>ownCloud marketplace integration and app management</description>
	<licence>AGPL</licence>
	<author>Thomas Müller, Florian Heidecke, Thomas Börger</author>
	<version>0.1.0</version>
	<default_enable/>
	<category>tools</category>
	<screenshot>https://raw.githubusercontent.com/owncloud/screenshots/master/market/screenshot.png</screenshot>
	<dependencies>
		<owncloud min-version="10.0" max-version="10.0" />
	</dependencies>
	<background-jobs>
		<job>OCA\Market\SyncJob</job>
	</background-jobs>
	<commands>
		<command>OCA\Market\Command\InstallApp</command>
		<command>OCA\Market\Command\ListApps</command>
		<command>OCA\Market\Command\UpgradeApp</command>
	</commands>
	<navigation role="admin">
		<route>market.page.index</route>
		<order>100</order>
	</navigation>
</info>
